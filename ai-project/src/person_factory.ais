var Person = {}
Person.create = function(opts) {
    var p = {}
    p.name = opts.name
    return p
}
var p = Person.create({ name: "Jack" })

var People = {}
People.create = function(opts) {
    var p = {}
    p.name = opts.name
    p.age = opts.age
    return p
}
var p2 = People.create({ name: "Jack", age: 50 })
